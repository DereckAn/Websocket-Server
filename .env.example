# Square Configuration
SQUARE_WEBHOOK_SIGNATURE_KEY=your_square_webhook_signature_key_here
SQUARE_ACCESS_TOKEN=your_square_access_token_here
SQUARE_APPLICATION_ID=your_square_application_id_here
SQUARE_ENVIRONMENT=sandbox # or production

# Server Configuration
WEBHOOK_PORT=3000
WEBSOCKET_PORT=3001
NODE_ENV=development # or production

# CORS Configuration
CORS_ORIGIN=http://localhost:3000 # For development
# CORS_ORIGIN=https://admin-mich-dereckans-projects.vercel.app # For production (can change)

# Logging
LOG_LEVEL=info # debug, info, warn, error

# Gomoku Game Configuration
MAX_ACTIVE_ROOMS=1000
ROOM_CLEANUP_INTERVAL=300000
INACTIVE_ROOM_TIMEOUT=1800000
AI_MAX_TIME_PER_MOVE=10000

# Rate Limiting
MAX_GAME_CREATIONS_PER_MINUTE=5
MAX_MOVES_PER_MINUTE=60

# Features disabled (for future use)
# DATABASE_URL=postgresql://... # Not used for Square orders
# EMAIL_SERVICE=disabled # No emails sent for terminal orders
# ANALYTICS=disabled # Not required currently