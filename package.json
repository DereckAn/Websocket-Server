{
  "name": "gomoku-game-server",
  "version": "1.0.0",
  "description": "High-performance Gomoku AI server with real-time multiplayer using MVC architecture",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "build": "bun build src/index.ts --outdir=dist --target=bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:unit": "bun test tests/unit/",
    "test:integration": "bun test tests/integration/",
    "test:coverage": "bun test --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "square": "^42.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "gomoku",
    "ai",
    "game",
    "websocket",
    "bun",
    "real-time",
    "minimax",
    "mvc"
  ],
  "author": "Your Name",
  "license": "MIT"
}
